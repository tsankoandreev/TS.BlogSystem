@model TS.BlogSystem.Core.Interfaces.IPagedList<TS.BlogSystem.Web.Models.CommentViewModel>

@for (int i = 0; i < Model.Count(); i++)
{
    @Html.DisplayFor(p => Model[i], "CommentItem")
}

<!-- Pagination ver.1-->
<pager-ajax pager-ajax-action="GetAll" pager-ajax-controller="Comments" pager-ajax-model="@Model" pager-ajax-update="#comments"></pager-ajax>
